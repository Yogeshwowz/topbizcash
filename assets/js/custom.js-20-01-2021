	$( "#contactForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#contactForm input[required],#contactForm textarea[required] ").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                //check invalid email
                var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
                if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                    $(this).css('border-color','#dc3545'); //change border color to red  
					proceed = false; //set do not proceed flag            
                }
				
        });
		
          if(proceed){
			H5_loading.show();
			var formData = $("#contactForm").serialize();
			$.ajax({
				type: "POST",
				url: baseURL+'home/contactForm',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					if(msgData==1){
						$('#contact-id').show();
						$('#contactForm input[type="text"],#contactForm input[type="email"],#contactForm textarea').val('');
					}
				
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });		
	$("#contactForm input[required]").keyup(function() { 
        $(this).css('border-color',''); 
	});	
	
	
	$(".numbervalidation").on("keypress keyup blur",function (event) {    
           $(this).val($(this).val().replace(/[^\d].+/, ""));
            if ((event.which < 48 || event.which > 57)) {
                event.preventDefault();
            }
        });
	
	
	$("#primarytypeOfEntityid").on('click', function () {
       var getvalue = $('#primarytypeOfEntityid').attr('rel');
	   
	   if(getvalue <= 1){
		   $('#alert2').removeClass('d-none');
		   //$('#alert2').focus();
		   $('html, body').animate({ scrollTop: $('#header').offset().top }, 'slow');
		   return false;
		   
	   }else{
		    $('#alert2').addClass('d-none');
	   }
    });
	
	
	$('body').on('change', '#primaryContactState', function() {
		var primaryContactState = $(this).val();
		if(primaryContactState=='Texas'){
			$('#primaryCountyDivmain').removeClass('by-default-hide');
			$('#primaryCountyDivtxt').addClass('by-default-hide');
			$("#primaryContactCountytxt").prop('required',false);
		     H5_loading.show();
		    $.ajax({
				type: "POST",
				url: baseURL+'home/primarycountry',
				data: {
					primaryContactState : primaryContactState
				},
				success: function(msgData) {
					H5_loading.hide();
					 $( "#primaryContactCounty" ).replaceWith(msgData);
					
				
				}
			});
		}else{
			$('#primaryCountyDivmain').addClass('by-default-hide');
			$('#primaryCountyDivtxt').removeClass('by-default-hide');
		    $( "#primaryContactCounty" ).replaceWith('<select size="0" id="primaryContactCounty" name="primaryContactCounty" class="form-control"  ></select>');
			$("#primaryContactCountytxt").prop('required',true);
		   
		}
	});
	
	
	
	
	
	$('body').on('change', '#businessState', function() {
		var businessState = $(this).val();
		
		if(businessState==44){
			$('#businessCountyDiv').removeClass('by-default-hide');
			$('#businessCountyDivtxt').addClass('by-default-hide');
			$("#businessCountytxt").prop('required',false);
			$("#businessCounty").prop('required',true);
		    
		}else{
			$('#businessCountyDiv').addClass('by-default-hide');
			$('#businessCountyDivtxt').removeClass('by-default-hide');
			$("#businessCounty").prop('required',false);
		    $("#businessCountytxt").prop('required',true);
		   
		}
	});
	
	
	
	$('body').on('change', '#previousOwnerState', function() {
		var previousOwnerState = $(this).val();
		
		if(previousOwnerState==44){
			$('#previousOwnerCountyDiv').removeClass('by-default-hide');
			$('#previousCountyDivtxt').addClass('by-default-hide');
			$("#previousCountytxt").prop('required',false);
			$("#previousOwnerCounty").prop('required',true);
		    
		}else{
			$('#previousOwnerCountyDiv').addClass('by-default-hide');
			$('#previousCountyDivtxt').removeClass('by-default-hide');
			$("#previousOwnerCounty").prop('required',false);
		    $("#previousCountytxt").prop('required',true);
		   
		}
	});
	
