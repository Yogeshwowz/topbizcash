	$("input[name='otherDistributionPoints']").on('click', function () {
       if($(this).prop("checked") == true){
			   if ($("input[name='otherDistributionPoints']").is(':checked')) {
				 var otherDistributionPoints =   $(this).val();
				  if(otherDistributionPoints==1){
					  $("#otherLocationDiv").removeClass("by-default-hide");
					  $("#locationStreetAddress,#locationCity,#locationState,#locationZip").prop('required',true);
					  $("#locationStreetAddress,#locationCity,#locationState,#locationZip").css('border-color','#dc3545');
					}else{
					   $("#otherLocationDiv").addClass("by-default-hide");
					   $("#locationStreetAddress,#locationCity,#locationState,#locationZip").prop('required',false);
					  $("#locationStreetAddress,#locationCity,#locationState,#locationZip").css('border-color','');
					  
				  }
				 
			   }
			   $("#error-radio-15").hide();
           }
    });
