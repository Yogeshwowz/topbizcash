	$("input[name='alcoholicBeverages']").on('click', function () {
       if($(this).prop("checked") == true){
			   if ($("input[name='alcoholicBeverages']").is(':checked')) {
				 var alcoholicBeverages =   $(this).val();
				  if(alcoholicBeverages==1){
					  $("#alcoholDiv").removeClass("by-default-hide");
					  
					}else{
					   $("#alcoholDiv").addClass("by-default-hide");
					}
				}
				$("#error-radio-1").hide();
					
           }
    });
	$("input[name='temporaryLocations']").on('click', function () {
       if($(this).prop("checked") == true){
			   if ($("input[name='temporaryLocations']").is(':checked')) {
				 var temporaryLocations =   $(this).val();
				 
				  if(temporaryLocations==1){
					  $("#temporaryLocationNameDiv,#periodInAttendanceDiv").removeClass("by-default-hide");
					   $("#temporaryLocationName,#periodInAttendance").prop('required',true);
					  
				  }else{
					  
					   $("#temporaryLocationNameDiv,#periodInAttendanceDiv").addClass("by-default-hide");
					   $("#temporaryLocationName,#periodInAttendance").prop('required',false);
						$('#temporaryLocationName,#periodInAttendance').css('border-color','');
					   
				  }
				 
			   }
			   $("#error-radio-2").hide();
           }
    });
	$("input[name='homeBased']").on('click', function () {
       if($(this).prop("checked") == true){
			   if ($("input[name='homeBased']").is(':checked')) {
				 var homeBased =   $(this).val();
				  if(homeBased==1){
					  $("#directionsDiv").removeClass("by-default-hide");
					  $("#directionsTextArea").prop('required',true);
					   $("#directionsTextArea").css('border-color','#dc3545');
					  
				  }else{
					   $("#directionsDiv").addClass("by-default-hide");
					   $("#directionsTextArea").prop('required',false);
					   $("#directionsTextArea").css('border-color','');
				  }
				 
			   }
           }
		     $("#error-radio-3").hide();
    });
