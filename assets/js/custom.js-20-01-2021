           }
    });
	
	
	
	$("#submitagg").on('click', function (event) {
		event.preventDefault();
       if($("input[name='agreeToTerms']").is(':checked')){
            window.location.href =baseURL+"payment";
            }else{
				$("#error-radio-1").show();
				
			}
    });
	
	
	
	
	$("input[name='alcoholicBeverages']").on('click', function () {
		//$amount = parseInt(109);
		//$Expedite  = parseInt(49);
	   $amount = parseFloat($('#basic_price').val());
	   $Expedite  = parseFloat($('#expedited_price').val());
       if($(this).prop("checked") == true){
		   if ($("input[name='alcoholicBeverages']").is(':checked')) {
			 var alcoholicBeverages =   $(this).val();
			  if(alcoholicBeverages==1){
				  $("#include-cls").removeClass("by-default-hide");
				  
				   $amount =  $Expedite + $amount;
				   $('#amount').val($amount);
				   
				}else{
					$('#amount').val($amount);
				    $("#include-cls").addClass("by-default-hide");
				   
				}
			 
		   }
              
            }
    });
	
});

function deleteResponsiblePerson($id){
	var baseURL = $('#baseURL').val();
	H5_loading.show();
	$.ajax({
				type: "POST",
				url: baseURL+'home/deleteprimaryForm',
				data: {
					id : $id
				},
				success: function(msgData) {
					H5_loading.hide();
					$('#card'+$id).remove();
					window.location.href=baseURL+"primary";
					return false;
					
				}
			});
}