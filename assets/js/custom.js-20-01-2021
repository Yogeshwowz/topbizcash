					H5_loading.hide();
					$('#primaryContactModal').modal('hide');
					$( "#accordion" ).append( msgData );
					$('#generalQuestionsForm input[type="text"],#generalQuestionsForm select,#generalQuestionsForm input[type="email"]').val('');
					window.location.href= baseURL+"supplierinformation/";
						return false;
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });
	
	

	
