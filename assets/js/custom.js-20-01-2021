	$( "#businessInformationForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#businessInformationForm input[required],#businessInformationForm radio[required],#businessInformationForm select[required]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
				//check invalid email
                var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
                if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                    $(this).css('border-color','#dc3545'); //change border color to red  
					proceed = false; //set do not proceed flag            
                }
				if ($("input[name='personalBank']").is(':checked')) {}else{
					$("#error-radio").show();
					proceed = false;
				}
				if ($("input[name='creditCardPayments']").is(':checked')) {}else{
					$("#error-radio-1").show();
					proceed = false;
				}
				
				if ($("input[name='involvedInMerger']").is(':checked')) {}else{
					$("input[name='involvedInMerger']").css('outline','#dc3545');
					$("#error-radio-2").show();
					 proceed = false;
				}
				if ($("input[name='businessLocation']").is(':checked')) {}else{
					$("input[name='businessLocation']").css('outline','#dc3545');
					$("#error-radio-3").show();
					 proceed = false;
				}
                
        });
		
          if(proceed){
			  
			H5_loading.show();
			var formData = $("#businessInformationForm").serialize();
			
			$.ajax({
				type: "POST",
				url: baseURL+'home/businessInformationForm',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					$('#primaryContactModal').modal('hide');
					$( "#accordion" ).append( msgData );
					$('#businessInformationForm input[type="text"],#businessInformationForm select,#businessInformationForm input[type="email"]').val('');
					window.location.href= baseURL +'generalquestions';
						return false;
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });
	
	
	$('body').on('keyup', '#businessInformationForm input[required]', function() {
		$(this).css('border-color',''); 
	});
	$('body').on('change', '#businessInformationForm select[required]', function() {
		$(this).css('border-color',''); 
	});
	
	
	$("input[name='personalBank']").on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-radio").hide();
            }
    });
	$("input[name='creditCardPayments']").on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-radio-1").hide();
            }
    });
	
	$("input[name='creditCardPayments']").on('click', function () {
       if($(this).prop("checked") == true){
		   if ($("input[name='creditCardPayments']").is(':checked')) {
			 var creditCardPayments =   $(this).val();
			  if(creditCardPayments==1){
				  $("#paymentProcessorDiv,#merchantIdentificationNumberDiv").removeClass("by-default-hide");
				  $("#onlinePaymentProcessor,#merchantIdentificationNumber").prop('required',true);
			      
			  }else{
				   $("#paymentProcessorDiv,#merchantIdentificationNumberDiv").addClass("by-default-hide");
				   $("#onlinePaymentProcessor,#merchantIdentificationNumber").prop('required',false);
			  }
			 
		   }
              
            }
    });
	
	
	
