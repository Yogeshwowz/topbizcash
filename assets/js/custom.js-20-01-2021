$(document).ready(function(){
	var baseURL = $('#baseURL').val();
	
	$( "#stateForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#stateForm select[required=true]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                
                
        });
		
          if(proceed){
			//var state =$('#state').val();
			var state = $( "#state option:selected" ).text();
			window.location.href= baseURL+"state/"+state;
			
        }
       return false;
		event.preventDefault();
		
    });		
	$("#stateForm select[required=true]").change(function() { 
        $(this).css('border-color',''); 
	});
	
	
	
	
$( "#registerForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#registerForm input[required=true],#registerForm radio[required=true],#registerForm select[required=true]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                //check invalid email
                var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
                if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                    $(this).css('border-color','#dc3545'); //change border color to red  
					proceed = false; //set do not proceed flag            
                }
				var pass = $('#password').val();
				var password_confirm= $('#password-confirm').val();
				if(pass!=password_confirm){
					$("#error-new-password").show();
					 proceed = false;
				}
				if ($("input[name='agreeTerms']").is(':checked')) {}else{
					$("input[name='agreeTerms']").css('outline','#dc3545');
					$("#error-checkbox").show();
					 proceed = false;
				}
				if ($("input[name='expedite']").is(':checked')) {}else{
					$("input[name='expedite']").css('outline','#dc3545');
					$("#error-radio").show();
					 proceed = false;
				}
        });
		
          if(proceed){
			H5_loading.show();
			var formData = $("#registerForm").serialize();
			$.ajax({
				type: "POST",
				url: baseURL+'home/userRegistration',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					if(msgData==1){
						$('#error-email').show();
						
					}else{
						window.location.href=baseURL+"organizationinformation";
						return false;
					}
				
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });		
	$("#registerForm input[required=true]").keyup(function() { 
        $(this).css('border-color',''); 
	});	
	
	$( "#registerFormId" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#registerFormId input[required=true],#registerFormId radio[required=true],#registerFormId select[required=true]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                //check invalid email
                var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
                if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                    $(this).css('border-color','#dc3545'); //change border color to red  
					proceed = false; //set do not proceed flag            
                }
				var pass = $('#password').val();
				var password_confirm= $('#password-confirm').val();
				if(pass!=password_confirm){
					$("#error-new-password").show();
					 proceed = false;
				}
				if ($("input[name='agreeTerms']").is(':checked')) {}else{
					$("input[name='agreeTerms']").css('outline','#dc3545');
					$("#error-checkbox").show();
					 proceed = false;
				}
				
        });
		
          if(proceed){
			H5_loading.show();
			var formData = $("#registerFormId").serialize();
			$.ajax({
				type: "POST",
				url: baseURL+'home/userRegistration',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					if(msgData==1){
						$('#error-email').show();
						
					}else{
						window.location.href=baseURL+"organizationinformation";
						return false;
					}
				
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });		
	$("#registerFormId input[required=true]").keyup(function() { 
        $(this).css('border-color',''); 
	});	
	
	
	$("#password-confirm").keyup(function() { 
      $("#error-new-password").hide();
	});
	$('#agreeTerms').on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-checkbox").hide();
            }
    });
	$("input[name='expedite']").on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-radio").hide();
            }
    });
	
	
	$( "#loginForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#loginForm input[required=true]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                //check invalid email
                var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
                if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                    $(this).css('border-color','#dc3545'); //change border color to red  
					proceed = false; //set do not proceed flag            
                }
				
        });
		
          if(proceed){
			H5_loading.show();
			var formData = $("#loginForm").serialize();
			$.ajax({
				type: "POST",
				url: baseURL+'home/userLogin',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					if(msgData==1){
						$('#error-email').show();
						
					}else{
						window.location.href=baseURL+"organizationinformation";
						return false;
					}
				
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });
	
	
	
	$("#typeOfEntity").change(function() { 
      var typeOfEntity =   $(this).val();
		if(typeOfEntity==1){
			$('#feinDiv,#Businessdiv,#texasFileNumberDiv').addClass("display-none");
			$('#SSNDiv,#fnamediv,#middlediv,#lnamediv').removeClass("display-none");
			$('#fnamediv,#middlediv,#lnamediv').removeClass("by-default-hide");
			$("#FEIN,#businessName").prop('required',false);
			$("#individualSSN,#fname,#lname").prop('required',true);
		}
		if(typeOfEntity==2 || typeOfEntity==3 || typeOfEntity==4 || typeOfEntity==5 || typeOfEntity==7){
			$('#feinDiv,#texasFileNumberDiv,#Businessdiv').removeClass("display-none");
			$('#SSNDiv').addClass("display-none");
			$('#fnamediv,#middlediv,#lnamediv').addClass("by-default-hide");
			$("#individualSSN,#fname,#lname").prop('required',false);
			$("#FEIN,#texasFileNumber,#businessName").prop('required',true);
		}
		if(typeOfEntity==6 || typeOfEntity==8 || typeOfEntity==9 || typeOfEntity==10 || typeOfEntity==11){
			$('#feinDiv,#Businessdiv').removeClass("display-none");
			$('#SSNDiv').addClass("display-none");
			$('#fnamediv,#middlediv,#lnamediv').addClass("by-default-hide");
			$('#fname,#lname').prop('required',false);
			$("#individualSSN,#texasFileNumber").prop('required',false);
			$("#FEIN,#businessName").prop('required',true);
		}
			
	});	
	$( "#texasOrganizationInformationForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#texasOrganizationInformationForm input[required],#texasOrganizationInformationForm radio[required],#texasOrganizationInformationForm select[required]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                /*if ($("input[name='involvedInMerger']").is(':checked')) {}else{
					$("input[name='involvedInMerger']").css('outline','#dc3545');
					$("#error-radio-1").show();
					 proceed = false;
				}
				if ($("input[name='businessLocation']").is(':checked')) {}else{
					$("input[name='businessLocation']").css('outline','#dc3545');
					$("#error-radio-2").show();
					 proceed = false;
				}*/
				if ($("input[name='TIN']").is(':checked')) {}else{
					$("#error-radio-1").show();
					proceed = false;
				}
        });
		
          if(proceed){
			  
			H5_loading.show();
			var formData = $("#texasOrganizationInformationForm").serialize();
			$.ajax({
				type: "POST",
				url: baseURL+'home/organizationInformationForm',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					if(msgData==1){
						window.location.href=baseURL+"primary";
						return false;
					}
				
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });		
	//$("#texasOrganizationInformationForm input[required]").keyup(function() {
		
	$('body').on('keyup', '#texasOrganizationInformationForm input[required]', function() {
		$(this).css('border-color',''); 
	});
	
	$("#texasOrganizationInformationForm select[required]").change(function() { 
        $(this).css('border-color',''); 
	});
	$("input[name='involvedInMerger']").on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-radio-2").hide();
            }
    });
	$("input[name='businessLocation']").on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-radio-3").hide();
            }
    });
	
	$("input[name='TIN']").on('click', function () {
       if($(this).prop("checked") == true){
              $("#error-radio-1").hide();
            }
    });
	
	$("input[name='TIN']").on('click', function () {
       if($(this).prop("checked") == true){
		   if ($("input[name='TIN']").is(':checked')) {
			 var TIN =   $(this).val();
			  if(TIN==1){
				  $("#texasFileNumberDiv").removeClass("by-default-hide");
				  $("#texasTIN").prop('required',true);
			      
			  }else{
				   $("#texasFileNumberDiv").addClass("by-default-hide");
				   $("#texasTIN").prop('required',false);
			  }
			 
		   }
              
            }
    });
	
	

	
	$( "#primaryForm" ).submit(function( event ){ //on form submit       
        var proceed = true;
		$("#primaryForm input[required],#primaryForm radio[required],#primaryForm select[required]").each(function(){
                $(this).css('border-color',''); 
                if(!$.trim($(this).val())){ //if this field is empty 
                    $(this).css('border-color','#dc3545'); //change border color to red   
					proceed = false; //set do not proceed flag
					$(this).focus();
                }
                //check invalid email
                var email_reg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/; 
                if($(this).attr("type")=="email" && !email_reg.test($.trim($(this).val()))){
                    $(this).css('border-color','#dc3545'); //change border color to red  
					proceed = false; //set do not proceed flag            
                }
        });
		
          if(proceed){
			  
			H5_loading.show();
			var formData = $("#primaryForm").serialize();
			$.ajax({
				type: "POST",
				url: baseURL+'home/primaryForm',
				data: {
					formdata : formData
				},
				success: function(msgData) {
					H5_loading.hide();
					$('#primaryContactModal').modal('hide');
					$( "#accordion" ).append( msgData );
					$('#primaryForm input[type="text"],#primaryForm input[type="email"],#primaryForm select,#primaryForm input[type="date"]').val('');
					window.location.href=baseURL+"primary";
						return false;
					
				
				}
			});
				
        }
       return false;
		event.preventDefault();
		
    });
	
