	$("input[name='brAlternateContact']").on('click', function () {
       if($(this).prop("checked") == true){
			   if ($("input[name='brAlternateContact']").is(':checked')) {
				 var brAlternateContact =   $(this).val();
				  if(brAlternateContact==1){
